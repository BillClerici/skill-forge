# SkillForge Environment Configuration
# Copy this file to .env and fill in your values

# Database Passwords
POSTGRES_PASSWORD=your_postgres_password_here
NEO4J_PASSWORD=your_neo4j_password_here
MONGO_PASSWORD=your_mongo_password_here
RABBITMQ_PASSWORD=your_rabbitmq_password_here

# JWT Secret (generate a secure random string)
JWT_SECRET=your_jwt_secret_key_here

# API Keys (add when needed)
ANTHROPIC_API_KEY=sk-ant-your-key-here
OPENAI_API_KEY=sk-your-key-here
STRIPE_API_KEY=sk_test_your-key-here

# External Services (add when needed)
STABLE_DIFFUSION_API_KEY=your_key_here
FIREBASE_PROJECT_ID=your_project_here

# Environment
ENVIRONMENT=development
DEBUG=true
